<div class="status">
    <div class="status_content">
        <a href="{{ vm.UserUrl|e }}">
            {{ vm.UserName|e }}
        </a>
        {{ vm.Body|e }}
    </div>
    <div>
        {% for image in vm.Images %}
            <img src="{{ image.url }}">
        {% endfor %}
    </div>
    <div>
        <a href="{{ vm.StatusUrl }}">
            {{ vm.CreatedAt|e }}
        </a>
    </div>
</div>
